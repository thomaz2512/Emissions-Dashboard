   "source": [
    "SELECT state_abbr,\n",
    "       SUM(CAST(\n",
    "         REPLACE(`GHG emissions mtons CO2e`, ',', '') \n",
    "         AS DOUBLE\n",
    "       )) AS Total_Emission\n",
    "FROM emissions_data\n",
    "GROUP BY state_abbr\n",
    "ORDER BY Total_Emission DESC\n",
    "LIMIT 10"
   ]
  }
