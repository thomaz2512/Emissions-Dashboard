{
  "datasets": [
    {
      "name": "09240ff7",
      "displayName": "Location Data",
      "queryLines": [
        "select latitude,\r\n",
        "longitude,\r\n",
        "'GHG emissions mtons CO2e' as Emissions\r\n",
        "from emission.default.emissions_data"
      ]
    },
    {
      "name": "60f8ee7d",
      "displayName": "Emission per Person",
      "queryLines": [
        "SELECT county_state_name,\r\n",
        "       population,\r\n",
        "       cast(\r\n",
        "         replace(`GHG emissions mtons CO2e`, ',', '') \r\n",
        "         AS double\r\n",
        "       ) / nullif(\r\n",
        "         cast(\r\n",
        "           replace(population, ',', '') \r\n",
        "           AS double\r\n",
        "         ), 0\r\n",
        "       ) AS Emissions_per_person\r\n",
        "FROM emission.default.emissions_data\r\n",
        "ORDER BY emissions_per_person DESC"
      ]
    },
    {
      "name": "5057c71a",
      "displayName": "Total Emission per state",
      "queryLines": [
        "SELECT state_abbr,\r\n",
        "       SUM(CAST(\r\n",
        "         REPLACE(`GHG emissions mtons CO2e`, ',', '') \r\n",
        "         AS DOUBLE\r\n",
        "       )) AS Total_Emission\r\n",
        "FROM emissions_data\r\n",
        "GROUP BY state_abbr\r\n",
        "ORDER BY Total_Emission DESC\r\n",
        "LIMIT 10"
      ],
      "catalog": "emission",
      "schema": "default"
    },
    {
      "name": "b7286c72",
      "displayName": "County Shaming",
      "queryLines": [
        "SELECT county_state_name,\r\n",
        "       population,\r\n",
        "       cast(\r\n",
        "         replace(`GHG emissions mtons CO2e`, ',', '') \r\n",
        "         AS DOUBLE) AS Total_Emissions\r\n",
        "FROM emission.default.emissions_data\r\n",
        "ORDER BY Total_Emissions DESC\r\n",
        "LIMIT 10"
      ],
      "catalog": "emission",
      "schema": "default"
    }
  ],
